{
  "name": "tmlabs",
  "version": "0.1.0",
  "description": "NodeJS SDK for TempicoLabs API",
  "main": "dist/tmlabs.umd.js",
  "jsnext:main": "dist/tmlabs.es.js",
  "files": [
    "src",
    "dist",
    "README.md"
  ],
  "scripts": {
    "lint:js": "eslint src tools",
    "lint:staged": "lint-staged",
    "compile": "babel src/ --out-dir dist/",
    "build": "npm run build:umd && npm run build:es && npm run build:browser",
    "build:umd": "rollup -c -f umd -o dist/tmlabs.umd.js",
    "build:browser": "rollup -c --environment production -f umd -o dist/tmlabs.min.js",
    "build:es": "rollup -c -f es -o dist/tmlabs.es.js",
    "test": "mocha",
    "test:watch": "npm run test -- --reporter min --watch",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:TempicoLabs/tmlabs.git"
  },
  "keywords": [
    "tempicolabs",
    "api",
    "sdk",
    "security",
    "es6"
  ],
  "author": "Maxim Maximov <root@4matikku.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TempicoLabs/tmlabs/issues"
  },
  "homepage": "https://github.com/TempicoLabs/tmlabs#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "env": {
      "production": {
        "presets": [
          "babili"
        ]
      },
      "test": {
        "plugins": [
          "istanbul",
          "rewire"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-external-helpers-2": "^6.3.13",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.0",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "rollup": "^0.43.0",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-babili": "^3.1.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0"
  },
  "dependencies": {
    "es6-promise": "^4.1.1",
    "isomorphic-fetch": "^2.2.1",
    "node-fetch": "^1.7.1",
    "validator": "^8.0.0"
  }
}
