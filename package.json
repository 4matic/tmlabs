{
  "name": "tmlabs",
  "version": "0.1.0",
  "description": "NodeJS SDK for TempicoLabs API",
  "main": "dist/tmlabs.umd.js",
  "jsnext:main": "dist/tmlabs.es.js",
  "files": [
    "bin",
    "src",
    "dist",
    "register.js",
    "README.md"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint:js": "eslint src tools",
    "lint:staged": "lint-staged",
    "build": "npm run build:umd && npm run build:es && npm run build:browser",
    "test": "mocha",
    "test:watch": "npm run test -- --reporter min --watch",
    "precommit": "lint-staged"
  },
  "bin": {
    "tmlabs": "./bin/tmlabs"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/4matic/tmlabs.git"
  },
  "keywords": [
    "tempicolabs",
    "api",
    "sdk",
    "security",
    "es6"
  ],
  "author": "Maxim Maximov <root@4matikku.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/4matic/tmlabs/issues"
  },
  "homepage": "https://github.com/4matic/tmlabs#readme",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "test": {
        "plugins": [
          "istanbul",
          "rewire"
        ]
      }
    }
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-rewire": "^1.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "cross-env": "^5.0.1",
    "eslint": "^4.1.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.0",
    "minimist": "^1.2.0",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "opn-cli": "^3.1.0",
    "rollup": "^0.43.0"
  }
}
