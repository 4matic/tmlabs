#!/usr/bin/env node
var minimist = require( 'minimist' );

var command = minimist( process.argv.slice( 2 ), {
	alias: {
		// Short options
		h: 'help',
		v: 'version',
		m: 'me',
		k: 'key'
	}
});

if ( command.help || ( process.argv.length <= 2 && process.stdin.isTTY ) ) {
	require( './showHelp' )();
} else if ( command.version ) {
	console.log( 'TempicoLabs SDK version ' + require( '../package.json' ).version );
} else {
  //run commands
}
